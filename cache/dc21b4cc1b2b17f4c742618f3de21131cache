            if($classAid.isGetAjax) return false;
            var item = $cmsLayer.getLoadItem("nine"+_this.page+url);
            if(item){
                callback(item);
                return false;
            }
            $classAid.isGetAjax = true;
            var indexLoad = $cmsLayer.load();
            return $.ajax({
                url:"?r=nine/listajax",
                type: 'GET',
                dataType: 'json',
                data:data ,
            }).error(function() {
                $cmsLayer.msg("服务器繁忙请稍后再试！");
                indexLoad.hide();
                $classAid.isGetAjax = false;
            }).done(function(data) {
                indexLoad.hide();
                $classAid.isGetAjax = false;                
                $cmsLayer.setLoadItem("nine_list"+_this.page,data);
                callback(data);
                return data;
            });
        },
        showClassAid : function(data){
            var _this = this;
            $classAid.ajaxUrl=data;
            if(!$classAid.pageStatus){
                if($classAid.isMsgLoad){
                    $cmsLayer.msg("已最后一页了！");
                }
                $classAid.isMsgLoad = false;
                return false;
            }
            var getClassAidListPromise = $classAid.getClassAidList(data,{page:$classAid.page,storey:1,n_id:'2',cac_id:this.cac_id},function(item){
                // if($classAid.page==1){
                //     $(".goods-new").html("");
                // }
                // 比对数据
                var dataItem = function(itemTwo){
                    var ret = $cmsLayer.copy(item.data.data);
                    $.each(itemTwo,function(index1, newItem) {
                        $.each(goodsAntData,function(index2, defItem) {
                            if( parseInt(newItem.id) == parseInt(defItem.id)){
                                ret[index1] = false;
                                return false;
                            }
                        });
                    });
                    var data = [];
                    $.each(ret,function(index,val){
                        if(val) data.push(val);
                    });
                    return data;
                }(item.data.data);
                _this.show_num=item.data.show_num;
                $classAid.appendItem(dataItem);
                _this.cac_id = item.data.cac_id;
                $classAid.pageStatus = item.data.pageStatus; 
                $classAid.page = $classAid.page + 1;
                
            });
        },
        appendItem : function(data){
            var _this = this;
            // 这里调用 cms_common 公用商品列表模板渲染
            var item =[];
            $.each(data,function(){
                if(_this.goodsNum<_this.show_num){
                    item.push(this);
                }else{
                    _this.pageStatus = false;
                    return false;
                }
                _this.goodsNum++;
            });
            console.log(this.goodsNum);
            var list = $($cmsApi.goodsListTpl(item,"/index.php?r=p/d","https://cmsstatic.ffquan.cn/")).addClass('loadPage'+$classAid.page);
            $(".goods-new").append(list);          
            $('.loadPage'+$classAid.page + " img").lazyload({
                effect: "fadeIn"
            }); 
        }
    };


    

    $(document).unbind('scroll').scroll(function(){
        if ($(document).scrollTop() >= $(document).height() - $(window).height() - 150 ) {
            $classAid.showClassAid($classAid.ajaxUrl,$classAid.page,"https://cmsstatic.ffquan.cn/");
        }
    });
    
    window.onload=function(){
        $classAid.appendItem(function(){
            var ret = [];
            $.each(goodsAntData,function(index, el) {
                if(index>2){
                    ret.push(el);
                }
            });
            return ret;
        }());
        $classAid.showClassAid();

    };
    $("img.lazy").lazyload({
        effect: "fadeIn"
    }); 
</script><!-- 主界面具体展示内容e -->


